# Cloning a repo
git clone https://github.com/jayme-anchate/repo

# Show local branches
git branch

# Show all branches
git branch -a

# To get all remote branches
git remote add upstream https://github.com/jayme-anchante/repo

# Fetch remote data
git fetch upstream

# Create local branch from remote branch
git checkout -b remote-branch upstream/remote-branch

# Push changes from local remote-branch to remote remote-branch
git add *
git commit -m 'this is a commit'
git push origin jayme
